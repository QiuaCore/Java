public class Ativ14 {
    public static void main(String[] args) {
        boolean temDinheiro = true, precoBaixo = false;

        boolean expr1 = temDinheiro;
        boolean expr2 = temDinheiro && precoBaixo;
System.out.println("Expr1 " + expr1 + " | Negação: " + !expr1);
System.out.println("Expr2 " + expr2 + " | Negação: " + !expr2);
        }
    }